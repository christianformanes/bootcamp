pipeline {
    agent any 
    
    tools {
        maven 'maven_3_6_1' 
    }
    
    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                    bat 'mvn clean compile'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}